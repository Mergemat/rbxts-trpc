{
	"name": "rbxts-trpc",
	"version": "1.0.2",
	"description": "Type-safe tRPC-style procedures and events for Roblox TypeScript.",
	"main": "dist/init.luau",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"default": "./dist/init.luau"
		},
		"./react": {
			"types": "./dist/react/index.d.ts",
			"default": "./dist/react/init.luau"
		}
	},
	"scripts": {
		"build": "rbxtsc",
		"model": "mkdir -p build && rojo build build.project.json -o build/rbxts-trpc.rbxm",
		"build:all": "bun run build && bun run model",
		"watch": "rbxtsc -w",
		"lint": "eslint . --ext .ts,.tsx",
		"lint:fix": "eslint . --ext .ts,.tsx --fix",
		"docs:dev": "bun run --cwd docs dev",
		"docs:build": "bun run --cwd docs build",
		"docs:lint": "bun run --cwd docs lint",
		"prepublishOnly": "bun run build"
	},
	"keywords": [
		"roblox",
		"roblox-ts",
		"trpc",
		"rpc",
		"types"
	],
	"author": "Mergemat",
	"license": "ISC",
	"repository": {
		"type": "git",
		"url": "https://github.com/Mergemat/rbxts-trpc.git"
	},
	"bugs": {
		"url": "https://github.com/Mergemat/rbxts-trpc/issues"
	},
	"homepage": "https://rbxts-trpc.vercel.app",
	"type": "commonjs",
	"types": "dist/index.d.ts",
	"typings": "dist/index.d.ts",
	"files": [
		"dist",
		"!dist/examples/**",
		"!dist/tests/**",
		"!**/*.tsbuildinfo"
	],
	"publishConfig": {
		"access": "public"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@rbxts/compiler-types": "^3.0.0-types.0",
		"@rbxts/types": "^1.0.907",
		"@typescript-eslint/eslint-plugin": "^8.55.0",
		"@typescript-eslint/parser": "^8.55.0",
		"eslint": "^9.39.2",
		"eslint-config-prettier": "^10.1.8",
		"eslint-plugin-prettier": "^5.5.5",
		"eslint-plugin-roblox-ts": "^1.3.1",
		"prettier": "^3.8.1",
		"roblox-ts": "^3.0.0",
		"typescript": "^5.9.3"
	},
	"dependencies": {
		"@rbxts/react": "^17.3.7-ts.1",
		"@rbxts/remo": "^1.5.3",
		"@rbxts/services": "^1.6.0",
		"@rbxts/t": "^3.2.1"
	}
}
